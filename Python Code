def is_part_of_series(lst):
    lst1 = []
    def calc(n):
        if n == 0 or n == 1:
            return n
        else:
            return(2*calc(n-1) - 2*calc(n-2))
    def make_list(n):
        for i in range(n):
            lst1.append(calc(i))
    max_num = 4*len(lst)
    make_list(max_num)
    print(lst1)
    flag = 0
    for l in lst:
        if not l in lst1:
            flag = 1
        else:
            pass
    if flag == 0:
        return True
    else:
        return False
